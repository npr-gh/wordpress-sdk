{
    "name": "wordproof/wordpress-sdk",
    "description": "WordPress SDK",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Yurii",
            "email": "yurii@wordproof.com"
        },
        {
            "name": "Marijn",
            "email": "marijn@wordproof.com"
        }
    ],
    "require": {
        "php": "^7.1.3||^8.0",
        "ext-json": "*",
        "wordproof/api-client": "^0.3.0",
        "typisttech/imposter-plugin": "^0.6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "WordProof\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WordProof\\SDK\\Tests\\": "tests"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "imposter": {
            "namespace": "WordProof\\SDK\\Vendor",
            "excludes": []
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
